account erik_lid@outlook.com
host smtp-mail.outlook.com
port 587
from erik_lid@outlook.com
user erik_lid@outlook.com
passwordeval "pass mail/erik_lid@outlook.com"
auth on
tls on
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
logfile /home/erik/.config/msmtp/msmtp.log
# tls_starttls

